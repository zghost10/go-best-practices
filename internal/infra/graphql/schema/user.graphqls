type User {
  id: ID!
  firstName: String!
  lastName: String!
  email: String!
}

type Query {
  users: [User!]!
  user(id: ID!): User!
}

input CreateUserInput {
  firstName: String!
  lastName: String!
  email: String!
  password: String!
}

type Mutation {
  createUser(input: CreateUserInput!): User!
}
